<Window x:Class="cmako.Statistics_Window"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:cmako"
        mc:Ignorable="d"
        Title="Анализ структуры теста" Height="450" Width="1500" Loaded="Window_Loaded">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>

        <DataGrid Name="dataGridView1" Height="Auto" Width="Auto" Margin="10,10,10,10" AutoGenerateColumns="True" ItemsSource="{Binding}" FontSize="16" Grid.Row="0"/>
    </Grid>
</Window>
